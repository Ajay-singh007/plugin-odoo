msgid "3DS authentication: "
msgstr "Autenticación 3DS: "

#: models/payment.py:264
msgid "YES"
msgstr "SÍ"

#: models/payment.py:265
msgid "3DS certificate: "
msgstr "Certificado 3DS: "

#: models/payment.py:267
msgid "NO"
msgstr "NO"

